<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.policebureauapp">
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ComplaintDetails"/>
        <activity android:name=".FirDetailActivity"></activity>
        <activity android:name=".FirStatisticsActivity" />
        <activity android:name=".NcrbFirActivity" />
        <activity android:name=".NcrbOffcialSignInActivity" />
        <activity android:name=".PermissionDetailActivity" />
        <activity android:name=".PermissionFormActivity" />
        <activity android:name=".FirRegistration" />
        <activity android:name=".PoliceSignInActivity" />
        <activity android:name=".PoliceActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>